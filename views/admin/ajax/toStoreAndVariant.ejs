

	<div class="form-group">
		<label for="toStoreId">Select To Store</label>
		<select name="toStoreId" class="form-control" id="toStoreId">
		  <option value="">Select To Store</option>
		  <% if(data.storeData && data.storeData.length>0){ %>
		  	<% for(let i = 0; i < data.storeData.length; i++){ %>
			<option value="<%=data.storeData[i]._id%>"><%= data.storeData[i].name%></option>
			<% 	} %>
		  <% } %>
		</select>
	</div>
	<div class="form-group">
		<label for="variantId">Variant</label>
		<select name="variantId" class="form-control" id="variantId">
		  <option value=""> Select Variant</option>
		  <% if(data.variantData && data.variantData.length>0){ %>
			<% for(let i = 0; i < data.variantData.length; i++){ %>
		  <option value="<%=data.variantData[i].id%>"><%= data.variantData[i].weight%></option>
		  <% 	} %>
		<% } %>
		</select>
	  </div>

