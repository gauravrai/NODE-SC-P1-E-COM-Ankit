<div class="content-wrapper">
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1>
                        <%=pageTitle%>
                    </h1>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item">
                            <a href="javascript:void(0)">
                                <%=moduleName%>
                            </a>
                        </li>
                        <li class="breadcrumb-item active">
                            <%=pageTitle%>
                        </li>
                    </ol>
                </div>
            </div>
        </div>
    </section>
    <section class="content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-12">
                    <div class="card card-primary card-outline">
                        <div class="card-header d-flex p-0">
                            <h3 class="card-title p-3">IP Address Information</h3>
                            <ul class="nav nav-pills ml-auto p-2">
                                <li class="nav-item"><a class="btn btn-block btn-secondary" href="<%=APPCONSTANT.ADMINCALLURL%>/manage_ipaddress">Back</a></li>
                            </ul>
                        </div>
                        <form role="form" id="editIpAddressdForm" method="post" enctype="multipart/form-data">
                            <div class="card-body">
                                <div class="form-group">
                                    <label for="brandId">Select Brand</label>
                                    <select name="brandId" class="form-control" id="brandId">
                        <option value="">Select Brand</option>
                        <% for(let i = 0; i < brandData.length; i++){ %>
                        <option value="<%=brandData[i].id%>" <% if(ipAddressData.brandId == brandData[i].id){ %> selected="selected" <% } %>><%=brandData[i].name%></option>
                        <% } %>
                        </select>
                                </div>
                                <div class="form-group">
                                    <label for="name">Name</label>
                                    <input type="text" name="ip_address" value="<%=ipAddressData.ipAddress?ipAddressData.ipAddress:''%>" class="form-control" id="ip_address" placeholder=" IP Address ">
                                </div>

                                <div class="form-group ">
                                    <label for="order ">Order</label>
                                    <input type="text " name="order" class="form-control" value="<%=ipAddressData.order?ipAddressData.order: ''%>" id="order" placeholder="Order">
                                </div>

                            </div>
                            <div class="card-footer">
                                <a href="<%=APPCONSTANT.ADMINCALLURL%>/manage_ipaddress" class="btn btn-default">Cancel</a>
                                <button type="submit" class="btn btn-primary">Submit</button>
                            </div>
                        </form>
                    </div>
                </div>
                <div class="col-md-6">

                </div>
            </div>
        </div>
    </section>
</div>