<div class="content-wrapper">
    <section class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1><%=pageTitle%></h1>
          </div>
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="javascript:void(0)"><%=moduleName%></a></li>
              <li class="breadcrumb-item active"><%=pageTitle%></li>
            </ol>
          </div>
        </div>
      </div>
    </section>
    <section class="content">
      <div class="container-fluid">
        <div class="row">
          <div class="col-md-12">
            <div class="card card-primary card-outline">
              <div class="card-header d-flex p-0">
                <h3 class="card-title p-3"><%=pageTitle%></h3>
                <ul class="nav nav-pills ml-auto p-2">
                  <% if(permissionData.add == '1'){ %>
                  <li class="nav-item" style="margin-right: 10px;"><a class="btn btn-block btn-primary" href="<%=APPCONSTANT.ADMINCALLURL%>/add_stock">Add Stock</a></li>
                  <li class="nav-item"><a class="btn btn-block btn-primary" href="<%=APPCONSTANT.ADMINCALLURL%>/transfer_stock">Transfer Stock</a></li>
                  <% } %>
                </ul>
              </div>
              <div class="card-header d-flex" id="searchHead">
                <h4> Search...</h4>
                <ul class="nav nav-pills ml-auto p-2">
                  <li class="nav-item"><i class="fas fa-search" id="global_search"> </i></li>
                </ul>
              </div>
              <div  class="card-header" id="searchBox" style="display: none !important;">
                <form method="GET">
                  <div class="row">
                    <div class="col-sm-3">
                      <div class="form-group">
                          <select name="store_id" id="store_id" class="form-control" onchange="this.form.submit()">
                            <option value="">Select Store</option>
                            <% for(let i = 0; i < storeData.length; i++){ %>
                              <option value="<%=storeData[i].id%>" <% if(req.query.store_id == storeData[i].id){ %>
                                selected="selected" <% } %>><%=storeData[i].name%></option>
                            <% } %>
                          </select>
                      </div>
                    </div>
                    <div class="col-sm-3">
                      <div class="form-group">
                        <input type="date" name="date_from" class="form-control" id="date_from" placeholder="(YYYY-MM-DD) From Date" value="<%= req.query.date_from ? req.query.date_from : ''%>">
                      </div>
                    </div>
                    <div class="col-sm-3">
                      <div class="form-group">
                        <input type="date" name="date_to" class="form-control" id="date_to" placeholder="(YYYY-MM-DD) To Date" value="<%= req.query.date_to ? req.query.date_to : ''%>">
                      </div>
                    </div>
                    <div class="col-sm-3">
                      <a href="<%=APPCONSTANT.ADMINCALLURL%>/manage_stock" class="btn btn-default">Cancel</a>
                      <button type="submit" class="btn btn-primary">Submit</button>
                    </div>
                  </div>
                </form>
              </div>
              <div class="card-body">
                <%- include ('../message/message') -%>
                <table id="stockTable" class="table table-striped table-bordered" style="width:100%">
                  <thead>
                      <tr>
                        <th>Product</th>
                        <th>Varient</th>
                        <th>Count</th>
                        <th>Store</th>
                        <th>Created date</th>
                        <th>Status</th>
                        <th>Action</th>
                      </tr>
                  </thead>
                  <tfoot>
                      <tr>
                        <th>Product</th>
                        <th>Varient</th>
                        <th>Count</th>
                        <th>Store</th>
                        <th>Created date</th>
                        <th>Status</th>
                        <th>Action</th>
                      </tr>
                  </tfoot>
              </table>
              </div>
            </div>
          </div>
          <div class="col-md-6">
  
          </div>
        </div>
      </div>
    </section>
  </div>