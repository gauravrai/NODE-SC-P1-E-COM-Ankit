<div class="content-wrapper">
    <section class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1><%=pageTitle%></h1>
          </div>
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="javascript:void(0)"><%=moduleName%></a></li>
              <li class="breadcrumb-item active"><%=pageTitle%></li>
            </ol>
          </div>
        </div>
      </div>
    </section>
    <section class="content">
      <div class="container-fluid">
        <div class="row">
          <div class="col-md-12">
            <div class="card card-primary card-outline">
              <div class="card-header d-flex p-0">
                <h3 class="card-title p-3">Store Information</h3>
                <ul class="nav nav-pills ml-auto p-2">
                  <li class="nav-item"><a class="btn btn-block btn-secondary" href="<%=APPCONSTANT.ADMINCALLURL%>/manage_store">Back</a></li>
                </ul>
              </div>
              <form role="form" id="editStoreForm" method="post">
                <div class="card-body">
                    <div class="form-group">
                        <label for="store">Store Name</label>
                        <input type="text" name="name" class="form-control" id="name" placeholder="Store Name" value="<%=storeData.name?storeData.name:''%>">
                    </div>
                    <div class="form-group">
                        <label for="stateId">Select State</label>
                        <select name="stateId" class="form-control" id="stateId" onchange="getLocationFields(this.value, 'city', 'get_city', 'Select City', '')">
                        <option value="">Select State</option>
                        <% for(let i = 0; i < stateData.length; i++){ %>
                        <option value="<%=stateData[i].id%>" <% if(storeData.stateId == stateData[i].id){ %> selected="selected" <% } %> ><%=stateData[i].name%></option>
                        <% } %>
                        </select>
                    </div>
                    <span id="cityData" class="locationFields">
                        <div class="form-group">
                        <label for="cityId">Select City</label>
                        <select name="cityId" class="form-control" id="cityId">
                            <option value="">Select City</option>
                            <% for(let i = 0; i < cityData.length; i++){ %>
                            <option value="<%=cityData[i].id%>" <% if(storeData.cityId == cityData[i].id){ %> selected="selected" <% } %> ><%=cityData[i].name%></option>
                            <% } %>
                        </select>
                        </div>
                    </span>
                    <div class="form-group">
                        <label for="address">Store Address</label>
                        <input type="hidden" name="id" value="<%=storeData.id?storeData.id:''%>">
                        <input type="text" name="address" class="form-control" id="address" placeholder="Store Address" value="<%=storeData.address?storeData.address:''%>">
                    </div>
                    <div class="form-group">
                      <label for="contactName">Contact Name</label>
                      <input type="text" name="contactName" class="form-control" id="contactName" placeholder="Contact Name" value="<%=storeData.contactName?storeData.contactName:''%>">
                    </div>
                    <div class="form-group">
                      <label for="contactNumber">Contact Number</label>
                      <input type="text" name="contactNumber" class="form-control" id="contactNumber" placeholder="Contact Number" value="<%=storeData.contactNumber?storeData.contactNumber:''%>">
                    </div>
                </div>
                <div class="card-footer">
                  <a href="<%=APPCONSTANT.ADMINCALLURL%>/manage_store" class="btn btn-default">Cancel</a>
                  <button type="submit" class="btn btn-primary">Submit</button>
                </div>
              </form>
            </div>
          </div>
          <div class="col-md-6">
  
          </div>
        </div>
      </div>
    </section>
  </div>