<div class="content-wrapper">
  <section class="content-header">
    <div class="container-fluid">
      <div class="row mb-2">
        <div class="col-sm-6">
          <h1><%=pageTitle%></h1>
        </div>
        <div class="col-sm-6">
          <ol class="breadcrumb float-sm-right">
            <li class="breadcrumb-item"><a href="javascript:void(0)"><%=moduleName%></a></li>
            <li class="breadcrumb-item active"><%=pageTitle%></li>
          </ol>
        </div>
      </div>
    </div>
  </section>
  <section class="content">
    <div class="container-fluid">
      <div class="row">
        <div class="col-md-12">
          <div class="card card-primary card-outline">
            <div class="card-header d-flex p-0">
              <h3 class="card-title p-3">Tower Information</h3>
              <ul class="nav nav-pills ml-auto p-2">
                <li class="nav-item"><a class="btn btn-block btn-secondary" href="<%=APPCONSTANT.ADMINCALLURL%>/manage_tower">Back</a></li>
              </ul>
            </div>
            <form role="form" id="editTowerForm" method="post">
              <div id="tableContent" style="padding: 0 17px 0 10px;">
                <div class="form-row">
                  <!-- <div class="card-body"> -->
                    <div class="form-group col-md-3">
                      <label for="stateId">Select State</label>
                      <select name="stateId" class="form-control" id="stateId" onchange="getLocationFields(this.value, 'city', 'get_city', 'Select City', '')">
                        <option value="">Select State</option>
                        <% for(let i = 0; i < stateData.length; i++){ %>
                        <option value="<%=stateData[i].id%>" <% if(towerData.stateId == stateData[i].id){ %> selected="selected" <% } %> ><%=stateData[i].name%></option>
                        <% } %>
                      </select>
                    </div>
                    <span id="cityData" class="locationFields col-md-3">
                      <div class="form-group">
                        <label for="cityId">Select City</label>
                        <select name="cityId" class="form-control" id="cityId">
                          <option value="">Select City</option>
                          <% for(let i = 0; i < cityData.length; i++){ %>
                          <option value="<%=cityData[i].id%>" <% if(towerData.cityId == cityData[i].id){ %> selected="selected" <% } %> ><%=cityData[i].name%></option>
                          <% } %>
                        </select>
                      </div>
                    </span>
                    <span id="pincodeData" class="locationFields col-md-3">
                      <div class="form-group">
                        <label for="pincodeId">Select Pincode</label>
                        <select name="pincodeId" class="form-control" id="pincodeId">
                          <option value="">Select Pincode</option>
                          <% for(let i = 0; i < pincodeData.length; i++){ %>
                          <option value="<%=pincodeData[i].id%>" <% if(towerData.pincodeId == pincodeData[i].id){ %> selected="selected" <% } %> ><%=pincodeData[i].pincode%></option>
                          <% } %>
                        </select>
                      </div>
                    </span>
                    <span id="areaData" class="locationFields col-md-3">
                      <div class="form-group">
                        <label for="areaId">Select Area</label>
                        <select name="areaId" class="form-control" id="areaId">
                          <option value="">Select Area</option>
                          <% for(let i = 0; i < areaData.length; i++){ %>
                          <option value="<%=areaData[i].id%>" <% if(towerData.areaId == areaData[i].id){ %> selected="selected" <% } %> ><%=areaData[i].name%></option>
                          <% } %>
                        </select>
                      </div>
                    </span>
                    <span id="societyData" class="locationFields col-md-3">
                      <div class="form-group">
                        <label for="societyId">Select Society</label>
                        <select name="societyId" class="form-control" id="societyId">
                          <option value="">Select Society</option>
                          <% for(let i = 0; i < societyData.length; i++){ %>
                          <option value="<%=societyData[i].id%>" <% if(towerData.societyId == societyData[i].id){ %> selected="selected" <% } %> ><%=societyData[i].name%></option>
                          <% } %>
                        </select>
                      </div>
                    </span>
                    <div class="form-group col-md-3">
                      <input type="hidden" name="id" value="<%=towerData.id?towerData.id:''%>">
                      <label for="name">Tower Name</label>
                      <input type="text" name="name" class="form-control" id="name" placeholder="Tower Name" value="<%=towerData.name?towerData.name:''%>">
                    </div>
                  <!-- </div> -->
                </div>
              </div>
              <div class="card-footer">
                <a href="<%=APPCONSTANT.ADMINCALLURL%>/manage_tower" class="btn btn-default">Cancel</a>
                <button type="submit" class="btn btn-primary">Submit</button>
              </div>
            </form>
          </div>
        </div>
        <div class="col-md-6">

        </div>
      </div>
    </div>
  </section>
</div>